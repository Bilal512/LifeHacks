<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.simbaone.lifehacks.presentation.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_carouselFragment"
            app:destination="@id/carouselFragment" />
    </fragment>
    <fragment
        android:id="@+id/carouselFragment"
        android:name="com.simbaone.lifehacks.presentation.carousal.CarouselFragment"
        android:label="CarouselFragment"
        tools:layout="@layout/fragment_carousal"/>
</navigation>